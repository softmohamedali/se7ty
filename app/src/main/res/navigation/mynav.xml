<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mynav"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.myassayment.ui.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" />
    <fragment
        android:id="@+id/accountFragment"
        android:name="com.example.myassayment.ui.AccountFragment"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" />
    <fragment
        android:id="@+id/bookingFragment"
        android:name="com.example.myassayment.ui.BookingFragment"
        android:label="fragment_booking"
        tools:layout="@layout/fragment_booking" />
    <dialog
        android:id="@+id/bottomChatFragment"
        android:name="com.example.myassayment.ui.BottomChatFragment"
        android:label="fragment_bottom_chat"
        tools:layout="@layout/fragment_bottom_chat" />
    <fragment
        android:id="@+id/emergencyFragment"
        android:name="com.example.myassayment.ui.EmergencyFragment"
        android:label="fragment_emergency"
        tools:layout="@layout/fragment_emergency" />
    <fragment
        android:id="@+id/servicesFragment"
        android:name="com.example.myassayment.ui.ServicesFragment"
        android:label="fragment_services"
        tools:layout="@layout/fragment_services" />
